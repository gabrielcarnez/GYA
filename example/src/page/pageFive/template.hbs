<h2>Page Five <span data-is-favourite></span></h2>
<div class="w3-col 12">
    <div class="w3-container">
        <div id="profile">
        	<label>Nombre</label>
        	<input
        		data-binding-with="data_4.profiles.{{data.profile.index}}.name" 
        		value="{{data.profile.name}}"
        		type="text" />
        	<label>Apellido</label>
        	<input 
        		data-binding-with="data_4.profiles.{{data.profile.index}}.lastName" 
        		value="{{data.profile.lastName}}"
        		type="text" />
        	<button data-id="{{data.profile.id}}" class="agregar">Agregar a favoritos</button>
        </div>
    </div>
</div>